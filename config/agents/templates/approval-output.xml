    <ApprovalTime>${ApprovalTime:-00:00:00}</ApprovalTime>
    <Amount>${Amount:-0.00}</Amount>
    <Currency>${Currency:-USD}</Currency>
    <Priority>${Priority:-Normal}</Priority>
