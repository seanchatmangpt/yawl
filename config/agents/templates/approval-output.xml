    <ApprovalID>${ApprovalID:-N/A}</ApprovalID>
    <RequestID>${RequestID:-N/A}</RequestID>
    <ApprovalStatus>${ApprovalStatus:-Approved}</ApprovalStatus>
    <ApprovalDecision>${ApprovalDecision:-Approve}</ApprovalDecision>
