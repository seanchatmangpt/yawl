		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ExpressionType">
		<xs:attribute name="query" type="yawl:XQueryType" use="required"/>
	</xs:complexType>

	<xs:complexType name="LayoutFactsType">
		<xs:sequence>
			<xs:element name="locale" type="yawl:LayoutLocaleType"/>
