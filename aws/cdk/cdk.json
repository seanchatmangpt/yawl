{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk.out",
      "venv",
      ".venv",
      "node_modules"
    ]
  },
  "context": {
    "availability_zones:account=true:region=us-east-1": [
      "us-east-1a",
      "us-east-1b",
      "us-east-1c"
    ],
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/aws-apigateway:usagePlanKeyOrderInsensitiveId": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-rds:lowercaseDbInstanceIdentifier": true,
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021_06": true,
    "@aws-cdk/aws-apigateway:requestValidatorUseRequestType": true,
    "@aws-cdk/aws-elasticloadbalancingv2:healthCheckDefaults": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretAsDefaultVersion": true
  }
}
