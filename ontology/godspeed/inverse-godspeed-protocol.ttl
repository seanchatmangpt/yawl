  inverse:direction "Backward (validate build artifacts)" ;

  # A2A Protocol: Build verification
  a2a:hasProtocol inverse:BuildValidationProtocol ;
  a2a:messagingStyle a2a:RequestReply ;
  a2a:concurrency a2a:Parallel ;

  # MCP Capabilities
