# ============================================================================
# INVERSE GODSPEED ORCHESTRATOR (Lead)
# Role: Coordinates the 5 inverse agents in backward sequence (Ω⁻¹→Q⁻¹→H⁻¹→Λ⁻¹→Ψ⁻¹)
# Principle: "Maximum Backward Safety (Zero State Corruption)"
# ============================================================================

inverse:InverseGodspeedOrchestrator a inverse:InverseLead ;
  rdfs:label "Inverse GODSPEED Orchestrator" ;
  rdfs:comment "Leads backward audit workflow, detects violations, suggests repairs" ;
  inverse:coordinatesAgents (
    inverse:OmegaInverseAgent
    inverse:ThetaInverseAgent
    inverse:EtaInverseAgent
    inverse:LambdaInverseAgent
    inverse:PsiInverseAgent
  ) ;
  inverse:phaseSequence "Ω⁻¹ → Q⁻¹ → H⁻¹ → Λ⁻¹ → Ψ⁻¹ (backward audit)" ;
  inverse:direction "Backward (detection & repair)" ;
  inverse:principle "Maximum Backward Safety (Zero State Corruption)" ;
  inverse:comparisonTarget godspeed:GodspeedOrchestrator ;

  # Orchestration protocol
  mcp:hasMCPServer mcp:InverseGodspeedOrchestratorMCPServer ;
  mcp:handlesTools mcp:inverse_audit_sequence, mcp:inverse_detect_violations, mcp:inverse_report ;
  mcp:resourceQuota [
    mcp:maxConcurrentTasks 1 ;
    mcp:maxContextTokens 250000 ;
    mcp:timeoutSeconds 3600
  ] .

mcp:InverseGodspeedOrchestratorMCPServer a mcp:Server ;
  rdfs:label "Inverse GODSPEED Orchestrator MCP Server" ;
  mcp:host "127.0.0.1" ;
  mcp:port 9110 ;
  mcp:protocol mcp:JSONRPCv2 ;
  mcp:authentication mcp:JWTBearer ;
  mcp:toolCount 3 .

# ============================================================================
# MESSAGE TYPES (Inverse Godspeed Protocol Messages)
# ============================================================================

inverse:AnalyzeHistoryRequest a a2a:MessageType ;
  rdfs:label "Analyze History Request" ;
  a2a:schema [ a2a:fields ( [ a2a:name "depth" ; a2a:type xsd:string ]
                              [ a2a:name "commit_range" ; a2a:type xsd:string ] ) ] .

inverse:DetectBrokenRequest a a2a:MessageType ;
  rdfs:label "Detect Broken Invariants Request" ;
  a2a:schema [ a2a:fields ( [ a2a:name "artifacts" ; a2a:type xsd:string ]
                              [ a2a:name "invariants" ; a2a:type xsd:string ] ) ] .

inverse:AuditAllRequest a a2a:MessageType ;
  rdfs:label "Audit All Request" ;
  a2a:schema [ a2a:fields ( [ a2a:name "patterns" ; a2a:type xsd:string ]
                              [ a2a:name "comprehensive" ; a2a:type xsd:boolean ] ) ] .

inverse:ValidateStateRequest a a2a:MessageType ;
  rdfs:label "Validate State Request" ;
  a2a:schema [ a2a:fields ( [ a2a:name "artifacts" ; a2a:type xsd:string ]
                              [ a2a:name "checksums" ; a2a:type xsd:string ] ) ] .

inverse:ValidateFactsRequest a a2a:MessageType ;
  rdfs:label "Validate Facts Request" ;
  a2a:schema [ a2a:fields ( [ a2a:name "fact_types" ; a2a:type xsd:string ]
                              [ a2a:name "checksum_validation" ; a2a:type xsd:boolean ] ) ] .

# ============================================================================
# INVERSE PHASES (Enumeration)
# ============================================================================

inverse:OmegaInversePhase a inverse:InversePhase ;
  rdfs:label "Ω⁻¹ Phase" ;
  inverse:order 1 ;
  inverse:principle "Trust but verify: audit all git operations" ;
  inverse:oppositeOf godspeed:OmegaPhase .

inverse:ThetaInversePhase a inverse:InversePhase ;
  rdfs:label "Q⁻¹ Phase" ;
  inverse:order 2 ;
  inverse:principle "Invariants once broken, now healed" ;
  inverse:oppositeOf godspeed:ThetaPhase .

inverse:EtaInversePhase a inverse:InversePhase ;
  rdfs:label "H⁻¹ Phase" ;
  inverse:order 3 ;
  inverse:principle "What forbidden patterns exist today?" ;
  inverse:oppositeOf godspeed:EtaPhase .

inverse:LambdaInversePhase a inverse:InversePhase ;
  rdfs:label "Λ⁻¹ Phase" ;
  inverse:order 4 ;
  inverse:principle "Can we rebuild from scratch and get identical results?" ;
  inverse:oppositeOf godspeed:LambdaPhase .

inverse:PsiInversePhase a inverse:InversePhase ;
  rdfs:label "Ψ⁻¹ Phase" ;
  inverse:order 5 ;
  inverse:principle "Do facts match the actual codebase state?" ;
  inverse:oppositeOf godspeed:PsiPhase .

