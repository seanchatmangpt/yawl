# ============================================================================
# COMPARISON: FORWARD vs BACKWARD
# ============================================================================

inverse:ComparisonMatrix a inverse:ComparisonPattern ;
  rdfs:label "GODSPEED Forward vs Backward Comparison" ;
  inverse:forward [
    inverse:sequence "Ψ→Λ→H→Q→Ω" ;
    inverse:principle "Maximum Forward Velocity" ;
    inverse:target "Zero Invariant Breakage" ;
    inverse:mode "Proactive (prevent violations)" ;
    inverse:timing "Before commit" ;
    inverse:authority "Monopoly (one source per phase)" ;
  ] ;
  inverse:backward [
    inverse:sequence "Ω⁻¹→Q⁻¹→H⁻¹→Λ⁻¹→Ψ⁻¹" ;
    inverse:principle "Maximum Backward Safety" ;
    inverse:target "Zero State Corruption" ;
    inverse:mode "Reactive (detect & repair)" ;
    inverse:timing "After commit (audit)" ;
    inverse:authority "Monopoly (one forensic source per phase)" ;
  ] .

# ============================================================================
# PHASE MATRIX: Forward Agent vs Backward Agent
# ============================================================================

inverse:PhaseCorrespondence a inverse:CorrespondencePattern ;
  rdfs:label "Correspondence Matrix" ;

  # Phase 1: Ψ ↔ Ψ⁻¹
  inverse:correspondence [
    inverse:forward godspeed:PsiAgent ;
    inverse:backward inverse:PsiInverseAgent ;
    inverse:forwardRole "Discovers facts from codebase" ;
    inverse:backwardRole "Validates facts match codebase" ;
    inverse:relationship "Symmetric verification" ;
  ] ;

  # Phase 2: Λ ↔ Λ⁻¹
  inverse:correspondence [
    inverse:forward godspeed:LambdaAgent ;
    inverse:backward inverse:LambdaInverseAgent ;
    inverse:forwardRole "Builds and compiles" ;
    inverse:backwardRole "Validates build state & reproducibility" ;
    inverse:relationship "Symmetric validation" ;
  ] ;

  # Phase 3: H ↔ H⁻¹
  inverse:correspondence [
    inverse:forward godspeed:EtaAgent ;
    inverse:backward inverse:EtaInverseAgent ;
    inverse:forwardRole "Enforces guards (fail-fast)" ;
    inverse:backwardRole "Audits violations (forensic)" ;
    inverse:relationship "Symmetric enforcement" ;
  ] ;

  # Phase 4: Q ↔ Q⁻¹
  inverse:correspondence [
    inverse:forward godspeed:ThetaAgent ;
    inverse:backward inverse:ThetaInverseAgent ;
    inverse:forwardRole "Verifies invariants" ;
    inverse:backwardRole "Repairs broken invariants" ;
    inverse:relationship "Symmetric restoration" ;
  ] ;

  # Phase 5: Ω ↔ Ω⁻¹
  inverse:correspondence [
    inverse:forward godspeed:OmegaAgent ;
    inverse:backward inverse:OmegaInverseAgent ;
    inverse:forwardRole "Ensures safe git operations" ;
    inverse:backwardRole "Audits git history (archaeology)" ;
    inverse:relationship "Symmetric audit" ;
  ] .
