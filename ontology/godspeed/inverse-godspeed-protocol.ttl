@prefix godspeed: <http://yawl.io/godspeed#> .
@prefix inverse: <http://yawl.io/godspeed/inverse#> .
@prefix a2a: <http://yawl.io/a2a#> .
@prefix mcp: <http://yawl.io/mcp#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ============================================================================
# INVERSE GODSPEED PROTOCOL — 5 Reverse/Remedial Agents
# ============================================================================
# Working BACKWARD through the pipeline (Ω⁻¹→Q⁻¹→H⁻¹→Λ⁻¹→Ψ⁻¹)
# Purpose: Detect violations after the fact, repair state, validate history
# Principle: "Maximum Backward Safety (Zero State Corruption)"
# ============================================================================

# Ω⁻¹ PHASE: GIT ARCHAEOLOGIST
# Role: Analyze git history, detect unsafe operations, rollback risk analysis
# Principle: "Trust but verify: audit all git operations"
# Concurrency: Sequential analysis (git history is linear)

inverse:OmegaInverseAgent a inverse:InverseAgent ;
  rdfs:label "Ω⁻¹ Agent (Git Archaeologist)" ;
  rdfs:comment "Analyzes git history, detects unsafe operations (force-push, amend), provides rollback guidance" ;
  inverse:phase inverse:OmegaInversePhase ;
  inverse:phaseName "Ω⁻¹ (Git Archaeology)" ;
  inverse:phaseOrder 1 ;
  inverse:principle "Trust but verify: audit all git operations" ;
  inverse:direction "Backward (from remote to local)" ;

  # A2A Protocol: Historical analysis
  a2a:hasProtocol inverse:GitArchaeologyProtocol ;
  a2a:messagingStyle a2a:RequestReply ;
  a2a:concurrency a2a:Sequential ;

  # MCP Capabilities
  mcp:hasMCPServer mcp:OmegaInverseMCPServer ;
  mcp:handlesTools mcp:omega_inv_analyze_history, mcp:omega_inv_detect_unsafe_ops, mcp:omega_inv_rollback_analysis ;
  mcp:resourceQuota [
    mcp:maxConcurrentTasks 1 ;
    mcp:maxContextTokens 200000 ;
    mcp:timeoutSeconds 600
  ] ;

  # Analysis outputs
  inverse:outputReports (
    "git-history-audit.json"
    "unsafe-operations-detected.json"
    "rollback-risk-analysis.json"
    "commit-integrity-report.json"
  ) ;

  # Endpoints
  a2a:endpoint [
    a2a:name "omega-inv/analyze-history" ;
    a2a:method a2a:POST ;
    a2a:messageType inverse:AnalyzeHistoryRequest ;
    a2a:responseTime 5000 ;
  ] ;
  a2a:endpoint [
    a2a:name "omega-inv/detect-unsafe-ops" ;
    a2a:method a2a:POST ;
    a2a:messageType inverse:DetectUnsafeOpsRequest ;
    a2a:responseTime 3000 ;
  ] ;
  a2a:endpoint [
    a2a:name "omega-inv/rollback-analysis" ;
    a2a:method a2a:POST ;
    a2a:messageType inverse:RollbackAnalysisRequest ;
    a2a:responseTime 2000 ;
  ] .

inverse:GitArchaeologyProtocol a a2a:Protocol ;
  rdfs:label "Ω⁻¹ Protocol (Git Archaeology)" ;
  a2a:messageFormat a2a:JSON ;
  a2a:encryption a2a:TLS12 ;
  a2a:historicalAnalysis [ inverse:depthScan "full_history" ; inverse:commitWindow "all" ] ;
  a2a:auditLogging [ inverse:forensics true ; inverse:immutable true ] .

mcp:OmegaInverseMCPServer a mcp:Server ;
  rdfs:label "Ω⁻¹ MCP Server (Git Archaeology)" ;
  mcp:host "127.0.0.1" ;
  mcp:port 9115 ;
  mcp:protocol mcp:JSONRPCv2 ;
  mcp:authentication mcp:JWTBearer ;
  mcp:toolCount 3 .

# ============================================================================
# Q⁻¹ PHASE: INVARIANT REPAIRMAN
# Role: Detect broken invariants, suggest repairs, validate fixes
# Principle: "Invariants once broken, now healed"
# Concurrency: Parallel repair suggestions

inverse:ThetaInverseAgent a inverse:InverseAgent ;
  rdfs:label "Q⁻¹ Agent (Invariant Repairman)" ;
  rdfs:comment "Detects broken invariants, suggests repairs, validates fix integrity" ;
  inverse:phase inverse:ThetaInversePhase ;
  inverse:phaseName "Q⁻¹ (Invariant Repair)" ;
  inverse:phaseOrder 2 ;
  inverse:principle "Invariants once broken, now healed" ;
  inverse:direction "Backward (from broken to fixed)" ;

  # A2A Protocol: Repair suggestions
  a2a:hasProtocol inverse:InvariantRepairProtocol ;
  a2a:messagingStyle a2a:RequestReply ;
  a2a:concurrency a2a:Parallel ;

  # MCP Capabilities
  mcp:hasMCPServer mcp:ThetaInverseMCPServer ;
  mcp:handlesTools mcp:theta_inv_detect_broken, mcp:theta_inv_suggest_repairs, mcp:theta_inv_validate_fix ;
  mcp:resourceQuota [
    mcp:maxConcurrentTasks 12 ;
    mcp:maxContextTokens 150000 ;
    mcp:timeoutSeconds 600
  ] ;

  # Analysis outputs
  inverse:outputReports (
    "broken-invariants-detected.json"
    "repair-suggestions.json"
    "fix-validation-report.json"
  ) ;

  # Endpoints
  a2a:endpoint [
    a2a:name "theta-inv/detect-broken" ;
    a2a:method a2a:POST ;
    a2a:messageType inverse:DetectBrokenRequest ;
    a2a:responseTime 5000 ;
  ] ;
  a2a:endpoint [
    a2a:name "theta-inv/suggest-repairs" ;
    a2a:method a2a:POST ;
    a2a:messageType inverse:SuggestRepairsRequest ;
    a2a:responseTime 3000 ;
  ] ;
  a2a:endpoint [
    a2a:name "theta-inv/validate-fix" ;
    a2a:method a2a:POST ;
    a2a:messageType inverse:ValidateFixRequest ;
    a2a:responseTime 2000 ;
  ] .

inverse:InvariantRepairProtocol a a2a:Protocol ;
  rdfs:label "Q⁻¹ Protocol (Invariant Repair)" ;
  a2a:messageFormat a2a:JSON ;
  a2a:encryption a2a:TLS12 ;
  a2a:semanticAnalysis [ inverse:depth "deep" ; inverse:contextTokens 150000 ] ;
  a2a:repairSuggestions [ inverse:automated true ; inverse:validated true ] .

mcp:ThetaInverseMCPServer a mcp:Server ;
  rdfs:label "Q⁻¹ MCP Server (Invariant Repair)" ;
  mcp:host "127.0.0.1" ;
  mcp:port 9114 ;
  mcp:protocol mcp:JSONRPCv2 ;
  mcp:authentication mcp:JWTBearer ;
  mcp:toolCount 3 .

