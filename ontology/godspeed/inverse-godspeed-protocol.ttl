inverse:FactValidationProtocol a a2a:Protocol ;
  rdfs:label "Ψ⁻¹ Protocol (Fact Validation)" ;
  a2a:messageFormat a2a:JSON ;
  a2a:encryption a2a:TLS12 ;
  a2a:validation [ inverse:checksums true ; inverse:reconciliation true ; inverse:completeness true ] ;
  a2a:parallelism [ inverse:maxParallel 32 ] .

mcp:PsiInverseMCPServer a mcp:Server ;
  rdfs:label "Ψ⁻¹ MCP Server (Fact Validation)" ;
