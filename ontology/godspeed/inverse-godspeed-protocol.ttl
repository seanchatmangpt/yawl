    a2a:responseTime 2000 ;
  ] ;
  a2a:endpoint [
    a2a:name "eta-inv/violation-timeline" ;
    a2a:method a2a:POST ;
    a2a:messageType inverse:ViolationTimelineRequest ;
    a2a:responseTime 3000 ;
  ] .

inverse:GuardAuditProtocol a a2a:Protocol ;
  rdfs:label "H⁻¹ Protocol (Guard Audit)" ;
  a2a:messageFormat a2a:JSON ;
  a2a:encryption a2a:TLS12 ;
  a2a:auditScope [ inverse:comprehensive true ; inverse:includeHistory true ] ;
  a2a:forensicDetailed [ inverse:patterns "all" ; inverse:locations true ; inverse:timestamps true ] .

mcp:EtaInverseMCPServer a mcp:Server ;
  rdfs:label "H⁻¹ MCP Server (Guard Audit)" ;
  mcp:host "127.0.0.1" ;
  mcp:port 9113 ;
  mcp:protocol mcp:JSONRPCv2 ;
  mcp:authentication mcp:JWTBearer ;
  mcp:toolCount 3 .

# ============================================================================
