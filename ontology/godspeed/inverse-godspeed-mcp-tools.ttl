    a schema:Thing ;
    schema:property [
      schema:name "artifacts" ;
      schema:description "Code artifacts to scan (files or module)" ;
      schema:valueType xsd:string ;
      schema:required true
    ] ;
    schema:property [
      schema:name "invariant_types" ;
      schema:description "Invariants to check (real_impl, no_mocks, etc.)" ;
      schema:valueType xsd:string ;
