@prefix godspeed: <http://yawl.io/godspeed#> .
@prefix inverse: <http://yawl.io/godspeed/inverse#> .
@prefix mcp: <http://yawl.io/mcp#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .

# ============================================================================
# INVERSE MCP TOOLS FOR BACKWARD GODSPEED PROTOCOL
# 5 Agents × 3 Tools per agent = 15 total inverse tools
# ============================================================================

# --------------------------------------------------------------------------
# OMEGA INVERSE AGENT TOOLS (Git Archaeology)
# --------------------------------------------------------------------------

mcp:omega_inv_analyze_history a mcp:Tool ;
  rdfs:label "Ω⁻¹ Analyze History" ;
  rdfs:comment "Analyze git history for unsafe operations, force pushes, amends" ;
  mcp:server mcp:OmegaInverseMCPServer ;
  inverse:phase inverse:OmegaInversePhase ;
  mcp:inputSchema [
    a schema:Thing ;
    schema:property [
      schema:name "commit_range" ;
      schema:description "Commit range to analyze (default: all)" ;
      schema:valueType xsd:string ;
      schema:required false
    ] ;
    schema:property [
      schema:name "depth" ;
      schema:description "Analysis depth (shallow, medium, deep)" ;
      schema:valueType xsd:string ;
      schema:required false
    ] ;
    schema:property [
      schema:name "look_for_unsafe" ;
      schema:description "Specifically search for unsafe operations?" ;
      schema:valueType xsd:boolean ;
      schema:required false
    ]
  ] ;
  mcp:outputSchema [
    a schema:Thing ;
    schema:property [
      schema:name "commits_analyzed" ;
      schema:valueType xsd:integer
    ] ;
    schema:property [
      schema:name "unsafe_operations_found" ;
      schema:description "Force pushes, amends, resets (JSON)" ;
      schema:valueType xsd:string
    ] ;
    schema:property [
      schema:name "audit_trail" ;
      schema:description "Complete audit trail" ;
      schema:valueType xsd:string
    ] ;
    schema:property [
      schema:name "risk_level" ;
      schema:description "LOW, MEDIUM, HIGH, CRITICAL" ;
      schema:valueType xsd:string
    ]
  ] ;
  mcp:rateLimit [ mcp:requestsPerSecond 1 ] ;
  inverse:forensic true ;
  inverse:comprehensive true .

mcp:omega_inv_detect_unsafe_ops a mcp:Tool ;
  rdfs:label "Ω⁻¹ Detect Unsafe Operations" ;
  rdfs:comment "Scan history for force-push, amend, rebase operations" ;
  mcp:server mcp:OmegaInverseMCPServer ;
  inverse:phase inverse:OmegaInversePhase ;
  mcp:inputSchema [
    a schema:Thing ;
    schema:property [
      schema:name "operation_types" ;
      schema:description "Types to search (force_push, amend, reset, rebase)" ;
      schema:valueType xsd:string ;
      schema:required false
    ] ;
    schema:property [
      schema:name "time_range" ;
      schema:description "Time window (default: all history)" ;
      schema:valueType xsd:string ;
      schema:required false
    ]
  ] ;
  mcp:outputSchema [
    a schema:Thing ;
    schema:property [
      schema:name "unsafe_ops_found" ;
      schema:valueType xsd:integer
    ] ;
    schema:property [
      schema:name "operations" ;
      schema:description "List of unsafe operations with details" ;
      schema:valueType xsd:string
    ] ;
    schema:property [
      schema:name "affected_commits" ;
      schema:description "Commits potentially affected" ;
      schema:valueType xsd:string
    ]
  ] ;
  mcp:rateLimit [ mcp:requestsPerSecond 10 ] ;
  inverse:failureMode "REPORT_FOUND_NOT_FAIL" .

mcp:omega_inv_rollback_analysis a mcp:Tool ;
  rdfs:label "Ω⁻¹ Rollback Analysis" ;
  rdfs:comment "Analyze rollback risk and provide safe rollback paths" ;
  mcp:server mcp:OmegaInverseMCPServer ;
  inverse:phase inverse:OmegaInversePhase ;
  mcp:inputSchema [
    a schema:Thing ;
    schema:property [
      schema:name "target_state" ;
      schema:description "Target commit or branch to rollback to" ;
      schema:valueType xsd:string ;
      schema:required true
    ] ;
    schema:property [
      schema:name "analyze_impact" ;
      schema:description "Analyze impact of rollback?" ;
      schema:valueType xsd:boolean ;
      schema:required false
    ]
  ] ;
  mcp:outputSchema [
    a schema:Thing ;
    schema:property [
      schema:name "rollback_risk" ;
      schema:description "LOW, MEDIUM, HIGH" ;
      schema:valueType xsd:string
    ] ;
    schema:property [
      schema:name "affected_branches" ;
      schema:description "Branches that would be affected" ;
      schema:valueType xsd:string
    ] ;
    schema:property [
      schema:name "safe_rollback_command" ;
      schema:description "Safe command to execute rollback" ;
      schema:valueType xsd:string
    ] ;
    schema:property [
      schema:name "side_effects" ;
      schema:description "Potential side effects" ;
      schema:valueType xsd:string
    ]
  ] ;
  mcp:rateLimit [ mcp:requestsPerSecond 5 ] .

# --------------------------------------------------------------------------
# THETA INVERSE AGENT TOOLS (Invariant Repair)
# --------------------------------------------------------------------------

