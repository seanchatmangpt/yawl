  ] ;
  mcp:outputSchema [
    a schema:Thing ;
    schema:property [
      schema:name "broken_count" ;
      schema:description "Number of broken invariants" ;
      schema:valueType xsd:integer
    ] ;
    schema:property [
      schema:name "broken_invariants" ;
      schema:description "List of broken invariants (JSON)" ;
      schema:valueType xsd:string
    ] ;
    schema:property [
      schema:name "severity" ;
      schema:description "CRITICAL, HIGH, MEDIUM, LOW" ;
      schema:valueType xsd:string
    ]
  ] ;
  mcp:rateLimit [ mcp:requestsPerSecond 20 ] ;
  inverse:forensic true .

