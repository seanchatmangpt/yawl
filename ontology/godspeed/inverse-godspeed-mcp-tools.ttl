mcp:theta_inv_validate_fix a mcp:Tool ;
  rdfs:label "Q⁻¹ Validate Fix" ;
  rdfs:comment "Validate that a proposed fix actually resolves the broken invariant" ;
  mcp:server mcp:ThetaInverseMCPServer ;
  inverse:phase inverse:ThetaInversePhase ;
  mcp:inputSchema [
    a schema:Thing ;
    schema:property [
      schema:name "fix_diff" ;
      schema:description "Unified diff of proposed fix" ;
      schema:valueType xsd:string ;
      schema:required true
    ] ;
    schema:property [
      schema:name "invariant" ;
      schema:description "Invariant to validate fix against" ;
      schema:valueType xsd:string ;
      schema:required true
    ]
  ] ;
