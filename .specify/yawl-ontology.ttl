# YAWL RDF Ontology v4.0
# Based on YAWL_Schema4.0.xsd
#
# This ontology formalizes the YAWL (Yet Another Workflow Language) specification
# using OWL 2 DL semantics with Dublin Core alignments for metadata.
#
# Namespaces:
#   yawls:  http://www.yawlfoundation.org/yawlschema#
#   owl:    http://www.w3.org/2002/07/owl#
#   rdfs:   http://www.w3.org/2000/01/rdf-schema#
#   rdf:    http://www.w3.org/1999/02/22-rdf-syntax-ns#
#   xsd:    http://www.w3.org/2001/XMLSchema#
#   dc:     http://purl.org/dc/elements/1.1/
#   dcterms: http://purl.org/dc/terms/
#   petri:  http://example.org/petrinet# (placeholder for Petri Net alignment)

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix yawls: <http://www.yawlfoundation.org/yawlschema#> .
@prefix : <http://www.yawlfoundation.org/yawlschema#> .

<http://www.yawlfoundation.org/yawlschema> a owl:Ontology ;
    rdfs:label "YAWL Ontology" ;
    rdfs:comment "OWL 2 DL ontology for YAWL workflow specifications based on Petri net semantics" ;
    owl:versionInfo "4.0" ;
    owl:imports <http://purl.org/dc/terms/> ;
    owl:imports <https://schema.org/> ;
    owl:imports <http://www.w3.org/ns/prov#> ;
    dcterms:created "2026-02-16"^^xsd:date ;
    dcterms:creator "YAWL Foundation" ;
    dcterms:license <https://www.gnu.org/licenses/lgpl-3.0.html> ;
    dcterms:description """
        This ontology captures the complete YAWL specification model including:
        - Workflow specifications and nets (Petri Net based)
        - Tasks with join/split control flow patterns (AND/XOR/OR)
        - Conditions (input/output conditions)
        - Decompositions (workflows, web service gateways)
        - Resourcing (participants, roles, privileges)
        - Data flow (parameters, variables, mappings)
        - Timers and temporal constraints
        - Configuration and layout information

        YAWL is an extension-only architecture: it extends public RDF ontologies
        (Schema.org, W3C PROV-O) rather than creating proprietary roots. See
        alignment profiles for formal semantic bridges to public domain standards.
    """ ;
    rdfs:seeAlso <http://www.yawlfoundation.org/yawlschema/schema-org-alignment> ;
    rdfs:seeAlso <http://www.yawlfoundation.org/yawlschema/prov-alignment> ;
    rdfs:seeAlso <http://www.yawlfoundation.org/yawlschema/fibo-alignment> ;
    rdfs:seeAlso <https://schema.org/documentation> ;
    rdfs:seeAlso <https://www.w3.org/TR/prov-o/> ;
    rdfs:seeAlso <https://spec.edmcouncil.org/fibo/ontology/> .

#################################################################
# ANNOTATION PROPERTIES (Dublin Core Alignment)
#################################################################

dc:title a owl:AnnotationProperty .
dc:creator a owl:AnnotationProperty .
dc:subject a owl:AnnotationProperty .
dc:description a owl:AnnotationProperty .
dc:contributor a owl:AnnotationProperty .
dc:coverage a owl:AnnotationProperty .
dc:identifier a owl:AnnotationProperty .
dcterms:valid a owl:AnnotationProperty .
dcterms:created a owl:AnnotationProperty .
dcterms:modified a owl:AnnotationProperty .

#################################################################
# CLASS HIERARCHY - Core Specification Model
#################################################################

# Root specification container
yawls:SpecificationSet a owl:Class ;
    rdfs:label "Specification Set" ;
    rdfs:comment "Container for one or more YAWL specifications" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:hasSpecification ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger
    ] .

# Primary specification class - maps from YAWLSpecificationFactsType
yawls:Specification a owl:Class ;
    rdfs:label "YAWL Specification" ;
    rdfs:comment "A complete YAWL workflow specification containing decompositions and metadata" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:uri ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasMetaData ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasDecomposition ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger
    ] ;
    dcterms:relation <http://purl.org/dc/dcmitype/Software> .

# Dublin Core metadata alignment
yawls:MetaData a owl:Class ;
    rdfs:label "Meta Data" ;
    rdfs:comment "Dublin Core aligned metadata for YAWL specifications" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty dcterms:title ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty dcterms:created ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty dcterms:valid ;
        owl:maxCardinality "2"^^xsd:nonNegativeInteger
    ] .

#################################################################
# DECOMPOSITION HIERARCHY
#################################################################

# Abstract base - maps from DecompositionFactsType
yawls:Decomposition a owl:Class ;
    rdfs:label "Decomposition" ;
    rdfs:comment "Abstract base class for all decomposable workflow elements" ;
    owl:hasKey ( yawls:id ) ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:id ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasInputParameter ;
        owl:minCardinality "0"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasOutputParameter ;
        owl:minCardinality "0"^^xsd:nonNegativeInteger
    ] .

# Workflow Net - maps from NetFactsType
yawls:WorkflowNet a owl:Class ;
    rdfs:label "Workflow Net" ;
    rdfs:comment "A YAWL workflow net containing process control elements" ;
    rdfs:subClassOf yawls:Decomposition , [
        a owl:Restriction ;
        owl:onProperty yawls:hasProcessControlElements ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasLocalVariable ;
        owl:minCardinality "0"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:isRootNet ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] ;
    skos:note "Corresponds to Petri Net workflow net with input and output conditions" .

# Web Service Gateway - maps from WebServiceGatewayFactsType
yawls:WebServiceGateway a owl:Class ;
    rdfs:label "Web Service Gateway" ;
    rdfs:comment "A decomposition that interfaces with external web services" ;
    rdfs:subClassOf yawls:Decomposition , [
        a owl:Restriction ;
        owl:onProperty yawls:hasYawlService ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasCodelet ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:externalInteraction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] .

#################################################################
# NET ELEMENT HIERARCHY
#################################################################

# Abstract base for all net elements
yawls:NetElement a owl:Class ;
    rdfs:label "Net Element" ;
    rdfs:comment "Abstract base class for elements in a workflow net" ;
    owl:hasKey ( yawls:id ) ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:id ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasFlowInto ;
        owl:minCardinality "0"^^xsd:nonNegativeInteger
    ] .

# Condition (places in Petri Net terms) - maps from ExternalConditionFactsType
yawls:Condition a owl:Class ;
    rdfs:label "Condition" ;
    rdfs:comment "A condition node in the workflow (corresponds to Petri Net place)" ;
    rdfs:subClassOf yawls:NetElement ;
    skos:note "Used for input conditions, output conditions, and intermediate conditions" .

# Input Condition - specific type of condition
yawls:InputCondition a owl:Class ;
    rdfs:label "Input Condition" ;
    rdfs:comment "Entry point of a workflow net" ;
    rdfs:subClassOf yawls:Condition ;
    owl:disjointWith yawls:OutputCondition .

# Output Condition - maps from OutputConditionFactsType
yawls:OutputCondition a owl:Class ;
    rdfs:label "Output Condition" ;
    rdfs:comment "Exit point of a workflow net" ;
    rdfs:subClassOf yawls:Condition .

# Task - maps from ExternalTaskFactsType
yawls:Task a owl:Class ;
    rdfs:label "Task" ;
    rdfs:comment "A work item in the workflow that may be executed by resources" ;
    rdfs:subClassOf yawls:NetElement , [
        a owl:Restriction ;
        owl:onProperty yawls:hasJoin ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasSplit ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasResourcing ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasTimer ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:decomposesTo ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] ;
    skos:note "Primary unit of work in YAWL workflows" .

# Multiple Instance Task - maps from MultipleInstanceExternalTaskFactsType
yawls:MultipleInstanceTask a owl:Class ;
    rdfs:label "Multiple Instance Task" ;
    rdfs:comment "A task that can have multiple concurrent instances" ;
    rdfs:subClassOf yawls:Task , [
        a owl:Restriction ;
        owl:onProperty yawls:minimum ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:maximum ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:threshold ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:creationMode ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

#################################################################
# CONTROL FLOW TYPES (AND/XOR/OR)
#################################################################

# Control Type - maps from ControlTypeType
yawls:ControlType a owl:Class ;
    rdfs:label "Control Type" ;
    rdfs:comment "Specifies the control flow behavior (AND, XOR, OR)" .

# Enumeration individuals for Control Type
yawls:AND a yawls:ControlType ;
    rdfs:label "AND" ;
    rdfs:comment "Parallel execution - all branches must complete" .

yawls:XOR a yawls:ControlType ;
    rdfs:label "XOR" ;
    rdfs:comment "Exclusive choice - exactly one branch executes" .

yawls:OR a yawls:ControlType ;
    rdfs:label "OR" ;
    rdfs:comment "Multi-choice - one or more branches may execute" .

# Join configuration
yawls:Join a owl:Class ;
    rdfs:label "Join" ;
    rdfs:comment "Join behavior for a task (how multiple incoming flows are synchronized)" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:code ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

# Split configuration
yawls:Split a owl:Class ;
    rdfs:label "Split" ;
    rdfs:comment "Split behavior for a task (how multiple outgoing flows are triggered)" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:code ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

#################################################################
# FLOW AND CONNECTIVITY
#################################################################

# Flow Into - maps from FlowsIntoType
yawls:FlowInto a owl:Class ;
    rdfs:label "Flow Into" ;
    rdfs:comment "Defines a flow from one element to another" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:nextElement ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasPredicate ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:isDefaultFlow ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] .

# Predicate - maps from PredicateType
yawls:Predicate a owl:Class ;
    rdfs:label "Predicate" ;
    rdfs:comment "XPath predicate for conditional flow" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:ordering ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:xpathExpression ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

#################################################################
# RESOURCING MODEL
#################################################################

# Resourcing - maps from ResourcingFactsType
yawls:Resourcing a owl:Class ;
    rdfs:label "Resourcing" ;
    rdfs:comment "Resource allocation and management for tasks" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:hasOffer ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasAllocate ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasStart ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

# Offer - maps from ResourcingOfferFactsType
yawls:Offer a owl:Class ;
    rdfs:label "Offer" ;
    rdfs:comment "Offer configuration for task distribution" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:initiator ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

# Allocate - maps from ResourcingAllocateFactsType
yawls:Allocate a owl:Class ;
    rdfs:label "Allocate" ;
    rdfs:comment "Allocation configuration for task assignment" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:initiator ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

# Distribution Set - maps from ResourcingDistributionSetFactsType
yawls:DistributionSet a owl:Class ;
    rdfs:label "Distribution Set" ;
    rdfs:comment "Set of resources for task distribution" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:hasInitialSet ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

# Resource Set - maps from ResourcingSetFactsType
yawls:ResourceSet a owl:Class ;
    rdfs:label "Resource Set" ;
    rdfs:comment "Collection of participants and roles" .

# Selector - maps from ResourcingSelectorFactsType
yawls:Selector a owl:Class ;
    rdfs:label "Selector" ;
    rdfs:comment "A resource selection mechanism (filter, allocator, constraint)" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:selectorName ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

# Privileges - maps from ResourcingPrivilegesFactsType
yawls:PrivilegeSet a owl:Class ;
    rdfs:label "Privilege Set" ;
    rdfs:comment "Set of resource privileges for a task" .

# Privilege enumeration (7 types from ResourcingPrivilegeType)
yawls:Privilege a owl:Class ;
    rdfs:label "Privilege" ;
    rdfs:comment "A resource management privilege" .

yawls:canSuspend a yawls:Privilege ;
    rdfs:label "Can Suspend" ;
    rdfs:comment "Allows suspending a work item" .

yawls:canReallocateStateless a yawls:Privilege ;
    rdfs:label "Can Reallocate Stateless" ;
    rdfs:comment "Allows stateless reallocation" .

yawls:canReallocateStateful a yawls:Privilege ;
    rdfs:label "Can Reallocate Stateful" ;
    rdfs:comment "Allows stateful reallocation" .

yawls:canDeallocate a yawls:Privilege ;
    rdfs:label "Can Deallocate" ;
    rdfs:comment "Allows deallocation" .

yawls:canDelegate a yawls:Privilege ;
    rdfs:label "Can Delegate" ;
    rdfs:comment "Allows delegation to another resource" .

yawls:canSkip a yawls:Privilege ;
    rdfs:label "Can Skip" ;
    rdfs:comment "Allows skipping the task" .

yawls:canPile a yawls:Privilege ;
    rdfs:label "Can Pile" ;
    rdfs:comment "Allows piling work items" .

# Initiator enumeration
yawls:Initiator a owl:Class ;
    rdfs:label "Initiator" ;
    rdfs:comment "Who initiates a resourcing action" .

yawls:systemInitiated a yawls:Initiator ;
    rdfs:label "System" ;
    rdfs:comment "System initiated action" .

yawls:userInitiated a yawls:Initiator ;
    rdfs:label "User" ;
    rdfs:comment "User initiated action" .

# Resource Type enumeration
yawls:ResourceType a owl:Class ;
    rdfs:label "Resource Type" ;
    rdfs:comment "Type of resource (participant or role)" .

yawls:Participant a yawls:ResourceType ;
    rdfs:label "Participant" ;
    rdfs:comment "Individual person resource" .

yawls:Role a yawls:ResourceType ;
    rdfs:label "Role" ;
    rdfs:comment "Role-based resource grouping" .

yawls:NonHumanResource a yawls:ResourceType ;
    rdfs:label "Non-Human Resource" ;
    rdfs:comment "Automated or system resource" .

yawls:NonHumanCategory a yawls:ResourceType ;
    rdfs:label "Non-Human Category" ;
    rdfs:comment "Category of non-human resources" .

# External Interaction enumeration
yawls:ExternalInteraction a owl:Class ;
    rdfs:label "External Interaction" ;
    rdfs:comment "How a task interacts with external systems" .

yawls:manual a yawls:ExternalInteraction ;
    rdfs:label "Manual" ;
    rdfs:comment "Manual interaction required" .

yawls:automated a yawls:ExternalInteraction ;
    rdfs:label "Automated" ;
    rdfs:comment "Automated interaction" .

#################################################################
# TIMER AND TEMPORAL CONSTRAINTS
#################################################################

# Timer - maps from TimerType
yawls:Timer a owl:Class ;
    rdfs:label "Timer" ;
    rdfs:comment "Timer configuration for tasks" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:useWorkdays ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] .

# Timer Trigger enumeration
yawls:TimerTrigger a owl:Class ;
    rdfs:label "Timer Trigger" ;
    rdfs:comment "When the timer starts" .

yawls:OnEnabled a yawls:TimerTrigger ;
    rdfs:label "On Enabled" ;
    rdfs:comment "Timer triggers when task is enabled" .

yawls:OnExecuting a yawls:TimerTrigger ;
    rdfs:label "On Executing" ;
    rdfs:comment "Timer triggers when task starts executing" .

# Timer Interval enumeration (8 time units)
yawls:TimerInterval a owl:Class ;
    rdfs:label "Timer Interval" ;
    rdfs:comment "Time unit for timer duration" .

yawls:YEAR a yawls:TimerInterval ;
    rdfs:label "Year" .

yawls:MONTH a yawls:TimerInterval ;
    rdfs:label "Month" .

yawls:WEEK a yawls:TimerInterval ;
    rdfs:label "Week" .

yawls:DAY a yawls:TimerInterval ;
    rdfs:label "Day" .

yawls:HOUR a yawls:TimerInterval ;
    rdfs:label "Hour" .

yawls:MIN a yawls:TimerInterval ;
    rdfs:label "Minute" .

yawls:SEC a yawls:TimerInterval ;
    rdfs:label "Second" .

yawls:MSEC a yawls:TimerInterval ;
    rdfs:label "Millisecond" .

# Timer Duration - maps from TimerDurationFactsType
yawls:TimerDuration a owl:Class ;
    rdfs:label "Timer Duration" ;
    rdfs:comment "Duration specification for a timer" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:ticks ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:interval ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

#################################################################
# DATA AND VARIABLES
#################################################################

# Variable - maps from VariableFactsType
yawls:Variable a owl:Class ;
    rdfs:label "Variable" ;
    rdfs:comment "A variable in a workflow net" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:variableName ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:dataType ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:index ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

# Input Parameter - maps from InputParameterFactsType
yawls:InputParameter a owl:Class ;
    rdfs:label "Input Parameter" ;
    rdfs:comment "Input parameter for a decomposition" ;
    rdfs:subClassOf yawls:Variable .

# Output Parameter - maps from OutputParameterFactsType
yawls:OutputParameter a owl:Class ;
    rdfs:label "Output Parameter" ;
    rdfs:comment "Output parameter from a decomposition" ;
    rdfs:subClassOf yawls:Variable , [
        a owl:Restriction ;
        owl:onProperty yawls:isMandatory ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:isCutThroughParam ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] .

# Variable Mapping - maps from VarMappingFactsType
yawls:VariableMapping a owl:Class ;
    rdfs:label "Variable Mapping" ;
    rdfs:comment "Maps data between variables using XQuery expressions" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:expression ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:mapsTo ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

# Expression - maps from ExpressionType
yawls:Expression a owl:Class ;
    rdfs:label "Expression" ;
    rdfs:comment "XQuery expression for data transformation" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:query ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

# Log Predicate - maps from LogPredicateFactsType
yawls:LogPredicate a owl:Class ;
    rdfs:label "Log Predicate" ;
    rdfs:comment "Logging configuration for data changes" .

#################################################################
# CONFIGURATION
#################################################################

# Configuration - maps from ConfigurationType
yawls:Configuration a owl:Class ;
    rdfs:label "Configuration" ;
    rdfs:comment "Configuration settings for variant management" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty yawls:hasJoinConfig ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasSplitConfig ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasNofiConfig ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty yawls:hasRemConfig ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] .

# Input Port Config - maps from InputPortConfigType
yawls:InputPortConfig a owl:Class ;
    rdfs:label "Input Port Configuration" ;
    rdfs:comment "Configuration for input ports" .

# Output Port Config - maps from OutputPortConfigType
yawls:OutputPortConfig a owl:Class ;
    rdfs:label "Output Port Configuration" ;
    rdfs:comment "Configuration for output ports" .

# Port Value enumeration
yawls:InputPortValue a owl:Class ;
    rdfs:label "Input Port Value" .

yawls:activated a yawls:InputPortValue .
yawls:blocked a yawls:InputPortValue .
yawls:hidden a yawls:InputPortValue .

yawls:OutputPortValue a owl:Class ;
    rdfs:label "Output Port Value" .

yawls:activatedOut a yawls:OutputPortValue .
yawls:blockedOut a yawls:OutputPortValue .

# Creation Mode enumeration
yawls:CreationModeValue a owl:Class ;
    rdfs:label "Creation Mode" .

yawls:staticMode a yawls:CreationModeValue ;
    rdfs:label "Static" .

yawls:dynamicMode a yawls:CreationModeValue ;
