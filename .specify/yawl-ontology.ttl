#################################################################
# VALIDITY CONSTRAINTS
#################################################################

# Every specification must have exactly one root net
yawls:Specification owl:hasKey ( yawls:uri ) .

# Tasks must have unique IDs within their net
yawls:Task owl:hasKey ( yawls:id ) .

# Conditions must have unique IDs within their net
yawls:Condition owl:hasKey ( yawls:id ) .

# Decompositions must have unique IDs
yawls:Decomposition owl:hasKey ( yawls:id ) .

# Variables must have unique names within their scope
yawls:Variable owl:hasKey ( yawls:variableName ) .

# FlowInto instances must reference valid next elements
yawls:FlowConnectsToValidElement a owl:Class ;
    owl:equivalentClass [
        a owl:Restriction ;
        owl:onProperty yawls:nextElement ;
        owl:someValuesFrom yawls:NetElement
    ] .

# Tasks with XOR/OR split must have predicates on outgoing flows
yawls:ConditionalSplitRequiresPredicates a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty [ owl:inverseOf yawls:hasFlowInto ] ;
        owl:someValuesFrom [
            a owl:Restriction ;
            owl:onProperty yawls:hasPredicate ;
            owl:someValuesFrom yawls:Predicate
        ]
    ] .

#################################################################
# PETRI NET ALIGNMENT NOTES
#################################################################

# The YAWL ontology is based on Petri Net semantics:
#
# yawls:Condition corresponds to Petri Net places
# yawls:Task corresponds to Petri Net transitions
# yawls:FlowInto corresponds to Petri Net arcs
#
# YAWL extends Petri Nets with:
# - Multiple instance tasks
# - Cancellation regions
# - OR-joins (non-local semantics)
# - Decomposition hierarchy
# - Resourcing model
# - Data flow

# End of YAWL Ontology
