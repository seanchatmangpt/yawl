<?xml version="1.0" encoding="UTF-8"?>
<!-- Mixed AND/XOR Patterns - Complex workflow combining parallel and exclusive structures -->
<specificationSet xmlns="http://www.citi.qut.edu.au/yawl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="Beta 4">
  <specification uri="WCP15_MixedSplitJoin">
    <metaData/>
    <schema xmlns="http://www.w3.org/2001/XMLSchema"/>
    <decomposition id="MixedSplitJoinNet" isRootNet="true" xsi:type="NetFactsType">
      <processControlElements>
        <inputCondition id="input">
          <flowsInto>
            <nextElementRef id="andSplit"/>
          </flowsInto>
        </inputCondition>
        <task id="andSplit">
          <flowsInto>
            <nextElementRef id="xorDecision"/>
          </flowsInto>
          <flowsInto>
            <nextElementRef id="taskC"/>
          </flowsInto>
          <join code="xor"/>
          <split code="and"/>
          <decomposesTo id="AndSplitDecomposition"/>
        </task>
        <task id="xorDecision">
          <flowsInto>
            <nextElementRef id="taskA"/>
            <predicate ordering="1">true()</predicate>
          </flowsInto>
          <flowsInto>
            <nextElementRef id="taskB"/>
            <isDefaultFlow/>
          </flowsInto>
          <join code="xor"/>
          <split code="xor"/>
          <decomposesTo id="XorDecisionDecomposition"/>
        </task>
        <task id="taskA">
          <flowsInto>
            <nextElementRef id="xorMerge"/>
          </flowsInto>
          <join code="xor"/>
          <split code="and"/>
          <decomposesTo id="TaskADecomposition"/>
        </task>
        <task id="taskB">
          <flowsInto>
            <nextElementRef id="xorMerge"/>
          </flowsInto>
          <join code="xor"/>
          <split code="and"/>
          <decomposesTo id="TaskBDecomposition"/>
        </task>
        <task id="xorMerge">
          <flowsInto>
            <nextElementRef id="andJoin"/>
          </flowsInto>
          <join code="xor"/>
          <split code="and"/>
          <decomposesTo id="XorMergeDecomposition"/>
        </task>
        <task id="taskC">
          <flowsInto>
            <nextElementRef id="andJoin"/>
          </flowsInto>
          <join code="xor"/>
          <split code="and"/>
          <decomposesTo id="TaskCDecomposition"/>
        </task>
        <task id="andJoin">
          <flowsInto>
            <nextElementRef id="output"/>
          </flowsInto>
          <join code="and"/>
          <split code="and"/>
          <decomposesTo id="AndJoinDecomposition"/>
        </task>
        <outputCondition id="output"/>
      </processControlElements>
    </decomposition>
    <decomposition id="AndSplitDecomposition" xsi:type="WebServiceGatewayFactsType"/>
    <decomposition id="XorDecisionDecomposition" xsi:type="WebServiceGatewayFactsType"/>
    <decomposition id="TaskADecomposition" xsi:type="WebServiceGatewayFactsType"/>
    <decomposition id="TaskBDecomposition" xsi:type="WebServiceGatewayFactsType"/>
    <decomposition id="XorMergeDecomposition" xsi:type="WebServiceGatewayFactsType"/>
    <decomposition id="TaskCDecomposition" xsi:type="WebServiceGatewayFactsType"/>
    <decomposition id="AndJoinDecomposition" xsi:type="WebServiceGatewayFactsType"/>
  </specification>
</specificationSet>
